{
  "name": "silverorange/mailchimp_api",
  "description": "MailChimp 1.x API wrapper.",
  "type": "library",
  "keywords": [ "mailchimp", "email", "campaign" ],
  "homepage": "https://github.com/silverorange/mailchimp-api",
  "license": "MIT",
  "authors": [
    {
      "name": "Isaac Grant",
      "email": "isaac@silverorange.com"
    }
  ],
  "support": {
    "issues": "https://github.com/silverorange/mailchimp-api/issues",
    "source": "https://github.com/silverorange/mailchimp-api"
  },
  "require": {
    "php": ">=5.1.5"
  },
  "autoload": {
    "psr-0": {
      "MailChimpAPI" : ""
    }
  },
  "include-path": [
    "./"
  ],
  "require-dev": {
    "silverorange/coding-standard": "^0.1.0"
  },
  "scripts": {
		"post-install-cmd": "./vendor/bin/phpcs --config-set installed_paths vendor/silverorange/coding-standard/src",
		"post-update-cmd": "./vendor/bin/phpcs --config-set installed_paths vendor/silverorange/coding-standard/src"
	}
}
